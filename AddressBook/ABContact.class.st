"
I represent a contact in an address book with a name, phone numbers, addresses, and emails.
"
Class {
	#name : #ABContact,
	#superclass : #Object,
	#instVars : [
		'firstName',
		'lastName',
		'phoneNumber',
		'address',
		'email',
		'description',
		'notes',
		'company',
		'lastUpdated',
		'avatar'
	],
	#category : #'AddressBook-Model'
}
